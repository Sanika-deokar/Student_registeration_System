    private void txtemailActionPerformed(java.awt.event.ActionEvent evt)                                         
    {                                             
        // TODO add your handling code here:
                                             
    String email = txtemail.getText();
    
    if (!email.equals(email.toLowerCase())) {
        JOptionPane.showMessageDialog(this, "Email address must be in lowercase.");
        txtemail.setText(email.toLowerCase()); // Optional: convert to lowercase
    } else if (!email.contains("@") || !email.contains(".")) {
        JOptionPane.showMessageDialog(this, "Invalid email address format.");
    
}

    }                                        

    private void txtphoneActionPerformed(java.awt.event.ActionEvent evt)                                         
    {                                             
        // TODO add your handling code here:
                                                
    String phoneNumber = txtphone.getText().trim();

    // Check if phone number is empty
    if (phoneNumber.isEmpty()) {
        JOptionPane.showMessageDialog(this, "Phone number cannot be empty!");
        return;
    }

    // Check if the phone number is numeric and has exactly 10 digits
    if (!phoneNumber.matches("\\d{10}")) {
        JOptionPane.showMessageDialog(this, "Please enter a valid 10-digit phone number!");
        
        return;
    }

    // If the validation passes, proceed with further logic
    JOptionPane.showMessageDialog(this, "Phone number is valid.");


    }                                        

