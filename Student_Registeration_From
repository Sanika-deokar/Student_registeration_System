    private void btnbrowseActionPerformed(java.awt.event.ActionEvent evt)                                          
    {                                              
        // TODO add your handling code here:
        JFileChooser chooser=new JFileChooser();
        chooser.showOpenDialog(null);
        File f=chooser.getSelectedFile();
        String path=f.getAbsolutePath();
        try
        {
            BufferedImage bi=ImageIO.read(new File(path));
            Image img=bi.getScaledInstance(134, 172,Image.SCALE_SMOOTH );
            ImageIcon icon =new ImageIcon(img);
            lblphoto.setIcon(icon);
        } catch (IOException ex)
        {
            Logger.getLogger(Registration.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                         

    private void btninsertActionPerformed(java.awt.event.ActionEvent evt)                                          
    {                                              
        // TODO add your handling code here:
        // TODO add your handling code here:
try (Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/student", "root", "sa123")) {

    // Inserting into the students table
    String studentQuery = "INSERT INTO students (name, photo, phone_number, email, birth_date, local_address, permanent_address, hometown, gender, college_name) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)";
    try (PreparedStatement studentStmt = con.prepareStatement(studentQuery, Statement.RETURN_GENERATED_KEYS)) {

        // Set student data
        studentStmt.setString(1, Txtfname.getText().trim() + " " + txtlname.getText().trim());  // Concatenate first and last name
        studentStmt.setNull(2, java.sql.Types.BLOB);  // Assuming photo is not available, setting it to NULL

        studentStmt.setString(3, txtphone.getText().trim());
        studentStmt.setString(4, txtemail.getText().trim());

        // Convert birth date (JCalendar or other date picker) to SQL Date
        if (dateofbrith.getDate() != null) {
            studentStmt.setDate(5, java.sql.Date.valueOf(dateofbrith.getDate().toInstant().atZone(ZoneId.systemDefault()).toLocalDate()));
        } else {
            studentStmt.setNull(5, java.sql.Types.DATE);
        }

        studentStmt.setString(6, txtalocaladress.getText().trim());
        studentStmt.setString(7, txtapermentaddress.getText().trim());

        // Determine selected gender
        String gender = null;
        if (rdiobtnmale.isSelected()) {
            gender = "Male";
        } else if (radiobtnfemale.isSelected()) {
            gender = "Female";
        }
        studentStmt.setString(9, gender);

        // Determine selected hometown
        String hometown = null;
        if (Rbtn1.isSelected()) {
            hometown = "Hometown1";  // Set appropriate value
        } else if (chkC.isSelected()) {
            hometown = "Hometown2";  // Set appropriate value
        } else if (Chkds.isSelected()) {
            hometown = "Hometown3";  // Set appropriate value
        } else if (chkads.isSelected()) {
            hometown = "Hometown4";  // Set appropriate value
        } else if (chkjava.isSelected()) {
            hometown = "Hometown5";  // Set appropriate value
        }
        studentStmt.setString(8, hometown);

        studentStmt.setString(10, txtcollegename.getText().trim());

        // Execute insert for student
        studentStmt.executeUpdate();

        // Fetch the generated student_id
        try (ResultSet generatedKeys = studentStmt.getGeneratedKeys()) {
            if (generatedKeys.next()) {
                int studentId = generatedKeys.getInt(1);  // Get auto-generated ID
                
                // Inserting into the enquiries table
                String enquiryQuery = "INSERT INTO enquiries (student_id, enquiry_date, subjects) VALUES (?, ?, ?)";
                try (PreparedStatement enquiryStmt = con.prepareStatement(enquiryQuery)) {
                    enquiryStmt.setInt(1, studentId);

                    // Convert enquiry date to SQL Date
                    if (calenderenquiry.getDate() != null) {
                        enquiryStmt.setDate(2, java.sql.Date.valueOf(calenderenquiry.getDate().toInstant().atZone(ZoneId.systemDefault()).toLocalDate()));
                    } else {
                        enquiryStmt.setNull(2, java.sql.Types.DATE);
                    }

                    enquiryStmt.setString(3, lblsubjects.getText().trim());  // Set subjects from label

                    // Execute insert for enquiry
                    enquiryStmt.executeUpdate();
                }
            } else {
                JOptionPane.showMessageDialog(this, "Failed to retrieve student ID.");
            }
        }
    }
    JOptionPane.showMessageDialog(this, "Inserted Successfully ");

} catch (SQLException e) {
    JOptionPane.showMessageDialog(this, "Error: " + e.getMessage());
}



    }                                         

